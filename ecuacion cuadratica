// practico 5.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include "math.h"
using namespace std;
void main ()

{
 float a,b,c,x,x1,x2,D;
  cout<<"ingrese a:";
  cin>>a,
  cout<<"ingrese b:";
  cin>>b;
  cout<<"ngrese c:";
  cin>>c;
  if (a==0)
  {
	  x=-c/b;
	  cout<<x;
  }
  else
      {
		  D=b*b-4*a*c;
          if (D>=0)
		  { x1=-b/2*a+sqrt(D)/2*a;
            x2=-b/2*a-sqrt(D)/2*a; 
			cout<<x1;
			cout<<x2;
		  }
		  else 
		  {
		  cout<<"solucion imaginarias";
		 }
		  cin>>x1;
		  cin>>x2;
  }
      
  getch ();
}
